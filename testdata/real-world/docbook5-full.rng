      <optional><attribute name="baseform"><data type="string"/></attribute></optional>
      <ref name="inlines"/>
    </element>
  </define>

  <!-- Index terms -->
  <define name="indexterm">
    <element name="indexterm">
      <ref name="db.common.attributes"/>
      <optional><attribute name="significance">
        <choice><value>normal</value><value>preferred</value></choice>
      </attribute></optional>
      <optional><attribute name="zone"><data type="IDREFS"/></attribute></optional>
      <optional><attribute name="type"><data type="string"/></attribute></optional>
      <optional><attribute name="class">
        <choice><value>singular</value><value>startofrange</value><value>endofrange</value></choice>
      </attribute></optional>
      <element name="primary">
        <ref name="db.common.attributes"/>
        <optional><attribute name="sortas"><data type="string"/></attribute></optional>
        <ref name="inlines"/>
      </element>
      <optional>
        <element name="secondary">
          <ref name="db.common.attributes"/>
          <optional><attribute name="sortas"><data type="string"/></attribute></optional>
          <ref name="inlines"/>
        </element>
      </optional>
      <optional>
        <element name="tertiary">
          <ref name="db.common.attributes"/>
          <optional><attribute name="sortas"><data type="string"/></attribute></optional>
          <ref name="inlines"/>
        </element>
      </optional>
      <zeroOrMore>
        <choice>
          <element name="see"><ref name="inlines"/></element>
          <element name="seealso"><ref name="inlines"/></element>
        </choice>
      </zeroOrMore>
    </element>
  </define>

  <define name="co">
    <element name="co">
      <attribute name="xml:id"><data type="ID"/></attribute>
      <optional><attribute name="linkends"><data type="IDREFS"/></attribute></optional>
      <optional><attribute name="label"><data type="string"/></attribute></optional>
    </element>
  </define>

  <define name="anchor">
    <element name="anchor">
      <attribute name="xml:id"><data type="ID"/></attribute>
    </element>
  </define>

  <define name="remark">
    <element name="remark"><ref name="db.common.attributes"/><ref name="inlines"/></element>
  </define>

  <define name="function">
    <element name="function"><ref name="db.common.attributes"/><ref name="inlines"/></element>
  </define>

</grammar>
