<?xml version="1.0" encoding="UTF-8"?>
<!--
  Extended DocBook 5 schema for stress testing.
  Expands the subset schema with:
  - Reference entries (refentry, refmeta, refnamediv, refsection)
  - Synopsis elements (funcsynopsis, cmdsynopsis)
  - Glossary, bibliography, index structures
  - Q&A sets (qandaset, qandaentry)
  - Callouts (calloutlist, callout, co)
  - Additional inlines (uri, userinput, computeroutput, prompt, etc.)
  - Index terms (indexterm, primary, secondary)
  - More interleave patterns for stress testing
-->
<grammar xmlns="http://relaxng.org/ns/structure/1.0"
         xmlns:db="http://docbook.org/ns/docbook"
         xmlns:xlink="http://www.w3.org/1999/xlink"
         datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes"
         ns="http://docbook.org/ns/docbook">

  <start>
    <choice>
      <ref name="article"/>
      <ref name="book"/>
      <ref name="refentry"/>
    </choice>
  </start>

  <!-- ═══════════════════════════════════════════════════════════════════════
       Common attribute groups
       ═══════════════════════════════════════════════════════════════════════ -->

  <define name="db.common.attributes">
    <optional><attribute name="xml:id"><data type="ID"/></attribute></optional>
    <optional><attribute name="xml:lang"><data type="language"/></attribute></optional>
    <optional><attribute name="xml:base"><data type="anyURI"/></attribute></optional>
    <optional><attribute name="version"><data type="string"/></attribute></optional>
    <optional><attribute name="role"><data type="string"/></attribute></optional>
    <optional><attribute name="revisionflag">
      <choice>
        <value>changed</value><value>added</value>
        <value>deleted</value><value>off</value>
      </choice>
    </attribute></optional>
    <optional><attribute name="arch"><data type="string"/></attribute></optional>
    <optional><attribute name="audience"><data type="string"/></attribute></optional>
    <optional><attribute name="condition"><data type="string"/></attribute></optional>
    <optional><attribute name="conformance"><data type="string"/></attribute></optional>
    <optional><attribute name="os"><data type="string"/></attribute></optional>
    <optional><attribute name="revision"><data type="string"/></attribute></optional>
    <optional><attribute name="security"><data type="string"/></attribute></optional>
    <optional><attribute name="userlevel"><data type="string"/></attribute></optional>
    <optional><attribute name="vendor"><data type="string"/></attribute></optional>
    <optional><attribute name="wordsize"><data type="string"/></attribute></optional>
    <optional><attribute name="annotations"><data type="string"/></attribute></optional>
    <zeroOrMore>
      <attribute>
        <anyName>
          <except>
            <nsName ns="http://docbook.org/ns/docbook"/>
            <nsName ns=""/>
            <nsName ns="http://www.w3.org/XML/1998/namespace"/>
            <nsName ns="http://www.w3.org/1999/xlink"/>
          </except>
        </anyName>
      </attribute>
    </zeroOrMore>
  </define>

  <define name="xlink.attribs">
    <optional><attribute name="xlink:href"><data type="anyURI"/></attribute></optional>
    <optional><attribute name="xlink:type"><data type="string"/></attribute></optional>
    <optional><attribute name="xlink:role"><data type="anyURI"/></attribute></optional>
    <optional><attribute name="xlink:title"><data type="string"/></attribute></optional>
    <optional><attribute name="xlink:show"><data type="string"/></attribute></optional>
    <optional><attribute name="xlink:actuate"><data type="string"/></attribute></optional>
  </define>

  <!-- ═══════════════════════════════════════════════════════════════════════
       Content models
       ═══════════════════════════════════════════════════════════════════════ -->

  <define name="inlines">
    <zeroOrMore>
      <choice>
        <text/>
        <ref name="emphasis"/>
        <ref name="link"/>
        <ref name="code"/>
        <ref name="literal"/>
        <ref name="command"/>
        <ref name="filename"/>
        <ref name="option"/>
        <ref name="replaceable"/>
        <ref name="varname"/>
        <ref name="classname"/>
        <ref name="methodname"/>
        <ref name="parameter"/>
        <ref name="type"/>
        <ref name="tag"/>
        <ref name="abbrev"/>
        <ref name="acronym"/>
        <ref name="citation"/>
        <ref name="footnote"/>
        <ref name="phrase"/>
        <ref name="subscript"/>
        <ref name="superscript"/>
        <ref name="trademark"/>
        <ref name="xref"/>
        <!-- New inlines -->
        <ref name="uri"/>
        <ref name="userinput"/>
        <ref name="computeroutput"/>
        <ref name="prompt"/>
        <ref name="systemitem"/>
        <ref name="package"/>
        <ref name="application"/>
        <ref name="guimenu"/>
        <ref name="guibutton"/>
        <ref name="guilabel"/>
        <ref name="keycap"/>
        <ref name="keycombo"/>
        <ref name="menuchoice"/>
        <ref name="citetitle"/>
        <ref name="quote"/>
        <ref name="foreignphrase"/>
        <ref name="wordasword"/>
        <ref name="firstterm"/>
        <ref name="glossterm"/>
        <ref name="indexterm"/>
        <ref name="co"/>
        <ref name="anchor"/>
        <ref name="remark"/>
        <ref name="function"/>
      </choice>
    </zeroOrMore>
  </define>

  <define name="blocks">
    <zeroOrMore>
      <choice>
        <ref name="para"/>
        <ref name="simpara"/>
        <ref name="formalpara"/>
        <ref name="programlisting"/>
        <ref name="screen"/>
        <ref name="literallayout"/>
        <ref name="note"/>
        <ref name="warning"/>
        <ref name="caution"/>
        <ref name="tip"/>
        <ref name="important"/>
        <ref name="itemizedlist"/>
        <ref name="orderedlist"/>
        <ref name="variablelist"/>
        <ref name="table"/>
        <ref name="informaltable"/>
        <ref name="example"/>
        <ref name="figure"/>
        <ref name="blockquote"/>
        <ref name="procedure"/>
        <ref name="sidebar"/>
        <ref name="mediaobject"/>
        <!-- New blocks -->
        <ref name="funcsynopsis"/>
        <ref name="cmdsynopsis"/>
        <ref name="synopsis"/>
        <ref name="calloutlist"/>
        <ref name="qandaset"/>
        <ref name="segmentedlist"/>
        <ref name="simplelist"/>
        <ref name="bridgehead"/>
        <ref name="informalexample"/>
        <ref name="informalfigure"/>
        <ref name="epigraph"/>
        <ref name="indexterm"/>
        <ref name="remark"/>
        <ref name="anchor"/>
      </choice>
    </zeroOrMore>
  </define>

  <!-- ═══════════════════════════════════════════════════════════════════════
       Top-level structures
       ═══════════════════════════════════════════════════════════════════════ -->

  <define name="article">
    <element name="article">
      <ref name="db.common.attributes"/>
      <optional>
        <attribute name="class">
          <choice>
            <value>faq</value><value>journalarticle</value>
            <value>productsheet</value><value>specification</value>
            <value>techreport</value><value>whitepaper</value>
          </choice>
        </attribute>
      </optional>
      <optional><ref name="info"/></optional>
      <ref name="blocks"/>
      <zeroOrMore><ref name="section"/></zeroOrMore>
      <optional><ref name="glossary"/></optional>
      <optional><ref name="bibliography"/></optional>
      <optional><ref name="index"/></optional>
    </element>
  </define>

  <define name="book">
    <element name="book">
      <ref name="db.common.attributes"/>
      <optional><ref name="info"/></optional>
      <optional><ref name="preface"/></optional>
      <zeroOrMore>
        <choice>
          <ref name="chapter"/>
          <ref name="appendix"/>
          <ref name="part"/>
          <ref name="reference"/>
        </choice>
      </zeroOrMore>
      <optional><ref name="glossary"/></optional>
      <optional><ref name="bibliography"/></optional>
      <optional><ref name="index"/></optional>
      <optional><ref name="colophon"/></optional>
    </element>
  </define>

  <define name="preface">
    <element name="preface">
      <ref name="db.common.attributes"/>
      <optional><ref name="info"/></optional>
      <optional><ref name="title"/></optional>
      <ref name="blocks"/>
    </element>
  </define>

  <define name="colophon">
    <element name="colophon">
      <ref name="db.common.attributes"/>
      <optional><ref name="info"/></optional>
      <optional><ref name="title"/></optional>
      <ref name="blocks"/>
    </element>
  </define>

  <define name="chapter">
    <element name="chapter">
      <ref name="db.common.attributes"/>
      <optional><ref name="info"/></optional>
      <optional><ref name="title"/></optional>
      <ref name="blocks"/>
      <zeroOrMore><ref name="section"/></zeroOrMore>
    </element>
  </define>

  <define name="appendix">
    <element name="appendix">
      <ref name="db.common.attributes"/>
      <optional><ref name="info"/></optional>
      <optional><ref name="title"/></optional>
      <ref name="blocks"/>
      <zeroOrMore><ref name="section"/></zeroOrMore>
    </element>
  </define>

  <define name="part">
    <element name="part">
      <ref name="db.common.attributes"/>
      <optional><ref name="info"/></optional>
      <optional><ref name="title"/></optional>
      <optional><ref name="partintro"/></optional>
      <zeroOrMore>
        <choice>
          <ref name="chapter"/>
          <ref name="reference"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>

  <define name="partintro">
    <element name="partintro">
      <ref name="db.common.attributes"/>
      <optional><ref name="title"/></optional>
      <ref name="blocks"/>
    </element>
  </define>

  <define name="section">
    <element name="section">
      <ref name="db.common.attributes"/>
      <optional><attribute name="label"><data type="string"/></attribute></optional>
      <optional><attribute name="status"><data type="string"/></attribute></optional>
      <optional><ref name="info"/></optional>
      <optional><ref name="title"/></optional>
      <ref name="blocks"/>
      <zeroOrMore><ref name="section"/></zeroOrMore>
    </element>
  </define>

  <!-- ═══════════════════════════════════════════════════════════════════════
       Reference entries (man pages)
       ═══════════════════════════════════════════════════════════════════════ -->

  <define name="reference">
    <element name="reference">
      <ref name="db.common.attributes"/>
      <optional><ref name="info"/></optional>
      <optional><ref name="title"/></optional>
      <optional><ref name="partintro"/></optional>
      <zeroOrMore><ref name="refentry"/></zeroOrMore>
    </element>
  </define>

  <define name="refentry">
    <element name="refentry">
      <ref name="db.common.attributes"/>
      <optional><ref name="info"/></optional>
      <optional><ref name="refmeta"/></optional>
      <ref name="refnamediv"/>
      <optional><ref name="refsynopsisdiv"/></optional>
      <zeroOrMore><ref name="refsection"/></zeroOrMore>
    </element>
  </define>

  <define name="refmeta">
    <element name="refmeta">
      <ref name="db.common.attributes"/>
      <interleave>
        <element name="refentrytitle"><ref name="inlines"/></element>
        <optional><element name="manvolnum"><text/></element></optional>
        <optional><element name="refmiscinfo">
          <ref name="db.common.attributes"/>
          <optional><attribute name="class"><data type="string"/></attribute></optional>
          <ref name="inlines"/>
        </element></optional>
      </interleave>
    </element>
  </define>

  <define name="refnamediv">
    <element name="refnamediv">
      <ref name="db.common.attributes"/>
      <zeroOrMore>
        <element name="refname"><ref name="inlines"/></element>
      </zeroOrMore>
      <element name="refpurpose"><ref name="inlines"/></element>
      <zeroOrMore>
        <element name="refclass"><ref name="inlines"/></element>
      </zeroOrMore>
    </element>
  </define>

  <define name="refsynopsisdiv">
    <element name="refsynopsisdiv">
      <ref name="db.common.attributes"/>
      <optional><ref name="title"/></optional>
      <ref name="blocks"/>
    </element>
  </define>

  <define name="refsection">
    <element name="refsection">
      <ref name="db.common.attributes"/>
      <optional><ref name="info"/></optional>
      <optional><ref name="title"/></optional>
      <ref name="blocks"/>
      <zeroOrMore><ref name="refsection"/></zeroOrMore>
    </element>
  </define>

  <!-- ═══════════════════════════════════════════════════════════════════════
       Synopsis elements
       ═══════════════════════════════════════════════════════════════════════ -->

  <define name="funcsynopsis">
    <element name="funcsynopsis">
      <ref name="db.common.attributes"/>
      <zeroOrMore>
        <choice>
          <ref name="funcsynopsisinfo"/>
          <ref name="funcprototype"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>

  <define name="funcsynopsisinfo">
    <element name="funcsynopsisinfo">
      <ref name="db.common.attributes"/>
      <ref name="inlines"/>
    </element>
  </define>

  <define name="funcprototype">
    <element name="funcprototype">
      <ref name="db.common.attributes"/>
      <element name="funcdef">
        <ref name="db.common.attributes"/>
        <zeroOrMore>
          <choice>
            <text/>
            <ref name="function"/>
            <ref name="type"/>
            <ref name="replaceable"/>
          </choice>
        </zeroOrMore>
      </element>
      <zeroOrMore>
        <choice>
          <ref name="paramdef"/>
          <element name="void"><empty/></element>
          <element name="varargs"><empty/></element>
        </choice>
      </zeroOrMore>
    </element>
  </define>

  <define name="paramdef">
    <element name="paramdef">
      <ref name="db.common.attributes"/>
      <optional><attribute name="choice">
        <choice><value>opt</value><value>req</value></choice>
      </attribute></optional>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="parameter"/>
          <ref name="type"/>
          <element name="funcparams"><text/></element>
          <ref name="replaceable"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>

  <define name="cmdsynopsis">
    <element name="cmdsynopsis">
      <ref name="db.common.attributes"/>
      <optional><attribute name="sepchar"><data type="string"/></attribute></optional>
      <optional><attribute name="cmdlength"><data type="string"/></attribute></optional>
      <zeroOrMore>
        <choice>
          <ref name="command"/>
          <element name="arg">
            <ref name="db.common.attributes"/>
            <optional><attribute name="choice">
              <choice><value>opt</value><value>req</value><value>plain</value></choice>
            </attribute></optional>
            <optional><attribute name="rep">
              <choice><value>norepeat</value><value>repeat</value></choice>
            </attribute></optional>
            <ref name="inlines"/>
          </element>
          <element name="group">
            <ref name="db.common.attributes"/>
            <optional><attribute name="choice">
              <choice><value>opt</value><value>req</value><value>plain</value></choice>
            </attribute></optional>
            <optional><attribute name="rep">
              <choice><value>norepeat</value><value>repeat</value></choice>
            </attribute></optional>
            <ref name="inlines"/>
          </element>
          <element name="sbr"><empty/></element>
        </choice>
      </zeroOrMore>
    </element>
  </define>

  <define name="synopsis">
    <element name="synopsis">
      <ref name="db.common.attributes"/>
      <optional><attribute name="label"><data type="string"/></attribute></optional>
      <ref name="inlines"/>
    </element>
  </define>

  <!-- ═══════════════════════════════════════════════════════════════════════
       Glossary, Bibliography, Index
       ═══════════════════════════════════════════════════════════════════════ -->

  <define name="glossary">
    <element name="glossary">
      <ref name="db.common.attributes"/>
      <optional><ref name="info"/></optional>
      <optional><ref name="title"/></optional>
      <ref name="blocks"/>
      <zeroOrMore>
        <choice>
          <ref name="glossdiv"/>
          <ref name="glossentry"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>

  <define name="glossdiv">
    <element name="glossdiv">
      <ref name="db.common.attributes"/>
      <optional><ref name="info"/></optional>
      <optional><ref name="title"/></optional>
      <ref name="blocks"/>
      <zeroOrMore><ref name="glossentry"/></zeroOrMore>
    </element>
  </define>

  <define name="glossentry">
    <element name="glossentry">
      <ref name="db.common.attributes"/>
      <optional><attribute name="sortas"><data type="string"/></attribute></optional>
      <ref name="glossterm"/>
      <optional><element name="acronym"><ref name="db.common.attributes"/><ref name="inlines"/></element></optional>
      <zeroOrMore>
        <element name="glossdef">
          <ref name="db.common.attributes"/>
          <ref name="blocks"/>
          <zeroOrMore>
            <element name="glossseealso">
              <ref name="db.common.attributes"/>
              <optional><attribute name="otherterm"><data type="IDREF"/></attribute></optional>
              <ref name="inlines"/>
            </element>
          </zeroOrMore>
        </element>
      </zeroOrMore>
      <zeroOrMore>
        <element name="glosssee">
          <ref name="db.common.attributes"/>
          <optional><attribute name="otherterm"><data type="IDREF"/></attribute></optional>
          <ref name="inlines"/>
        </element>
      </zeroOrMore>
    </element>
  </define>

  <define name="bibliography">
    <element name="bibliography">
      <ref name="db.common.attributes"/>
      <optional><ref name="info"/></optional>
      <optional><ref name="title"/></optional>
      <ref name="blocks"/>
      <zeroOrMore>
        <choice>
          <ref name="bibliodiv"/>
          <ref name="biblioentry"/>
          <ref name="bibliomixed"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>

  <define name="bibliodiv">
    <element name="bibliodiv">
      <ref name="db.common.attributes"/>
      <optional><ref name="info"/></optional>
      <optional><ref name="title"/></optional>
      <ref name="blocks"/>
      <zeroOrMore>
        <choice>
          <ref name="biblioentry"/>
          <ref name="bibliomixed"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>

  <define name="biblioentry">
    <element name="biblioentry">
      <ref name="db.common.attributes"/>
      <interleave>
        <optional><ref name="title"/></optional>
        <zeroOrMore><ref name="author"/></zeroOrMore>
        <optional><ref name="pubdate"/></optional>
        <optional><element name="publisher">
          <ref name="db.common.attributes"/>
          <element name="publishername"><ref name="inlines"/></element>
          <optional><element name="address"><ref name="inlines"/></element></optional>
        </element></optional>
        <optional><element name="isbn"><text/></element></optional>
        <optional><element name="issn"><text/></element></optional>
        <optional><element name="biblioid">
          <ref name="db.common.attributes"/>
          <optional><attribute name="class"><data type="string"/></attribute></optional>
          <text/>
        </element></optional>
        <optional><ref name="abstract"/></optional>
        <zeroOrMore><ref name="indexterm"/></zeroOrMore>
      </interleave>
    </element>
  </define>

  <define name="bibliomixed">
    <element name="bibliomixed">
      <ref name="db.common.attributes"/>
      <ref name="inlines"/>
    </element>
  </define>

  <define name="index">
    <element name="index">
      <ref name="db.common.attributes"/>
      <optional><ref name="info"/></optional>
      <optional><ref name="title"/></optional>
      <ref name="blocks"/>
      <zeroOrMore>
        <element name="indexdiv">
          <ref name="db.common.attributes"/>
          <optional><ref name="title"/></optional>
          <ref name="blocks"/>
        </element>
      </zeroOrMore>
    </element>
  </define>

  <!-- ═══════════════════════════════════════════════════════════════════════
       Info block (with heavy interleave)
       ═══════════════════════════════════════════════════════════════════════ -->

  <define name="info">
    <element name="info">
      <ref name="db.common.attributes"/>
      <interleave>
        <optional><ref name="title"/></optional>
        <optional><ref name="subtitle"/></optional>
        <optional><element name="titleabbrev"><ref name="inlines"/></element></optional>
        <zeroOrMore><ref name="author"/></zeroOrMore>
        <zeroOrMore><ref name="editor"/></zeroOrMore>
        <zeroOrMore><ref name="othercredit"/></zeroOrMore>
        <optional><ref name="pubdate"/></optional>
        <optional><ref name="date"/></optional>
        <optional><ref name="copyright"/></optional>
        <optional><ref name="abstract"/></optional>
        <zeroOrMore><ref name="keywordset"/></zeroOrMore>
        <zeroOrMore><ref name="subjectset"/></zeroOrMore>
        <optional><ref name="releaseinfo"/></optional>
        <optional><ref name="revhistory"/></optional>
        <optional><ref name="legalnotice"/></optional>
        <optional>
          <element name="edition"><ref name="inlines"/></element>
        </optional>
        <optional>
          <element name="productname">
            <ref name="db.common.attributes"/>
            <optional><attribute name="class"><data type="string"/></attribute></optional>
            <ref name="inlines"/>
          </element>
        </optional>
        <optional><element name="productnumber"><ref name="inlines"/></element></optional>
        <optional><element name="volumenum"><ref name="inlines"/></element></optional>
        <optional><element name="issuenum"><ref name="inlines"/></element></optional>
        <optional><element name="collab"><ref name="db.common.attributes"/>
          <element name="orgname"><ref name="inlines"/></element>
        </element></optional>
        <zeroOrMore><ref name="indexterm"/></zeroOrMore>
      </interleave>
    </element>
  </define>

  <define name="title">
    <element name="title"><ref name="db.common.attributes"/><ref name="inlines"/></element>
  </define>

  <define name="subtitle">
    <element name="subtitle"><ref name="db.common.attributes"/><ref name="inlines"/></element>
  </define>

  <define name="author">
    <element name="author">
      <ref name="db.common.attributes"/>
      <interleave>
        <optional>
          <element name="personname">
            <ref name="db.common.attributes"/>
            <interleave>
              <optional><element name="firstname"><text/></element></optional>
              <optional><element name="surname"><text/></element></optional>
              <optional><element name="othername"><text/></element></optional>
              <optional><element name="honorific"><text/></element></optional>
              <optional><element name="lineage"><text/></element></optional>
            </interleave>
          </element>
        </optional>
        <zeroOrMore>
          <element name="affiliation">
            <ref name="db.common.attributes"/>
            <optional><element name="orgname"><ref name="inlines"/></element></optional>
            <optional><element name="jobtitle"><ref name="inlines"/></element></optional>
            <optional><element name="address"><ref name="inlines"/></element></optional>
          </element>
        </zeroOrMore>
        <optional><element name="email"><ref name="inlines"/></element></optional>
        <optional><element name="uri"><ref name="inlines"/></element></optional>
      </interleave>
    </element>
  </define>

  <define name="editor">
    <element name="editor">
      <ref name="db.common.attributes"/>
      <optional>
        <element name="personname">
          <ref name="db.common.attributes"/>
          <interleave>
            <optional><element name="firstname"><text/></element></optional>
            <optional><element name="surname"><text/></element></optional>
          </interleave>
        </element>
      </optional>
    </element>
  </define>

  <define name="othercredit">
    <element name="othercredit">
      <ref name="db.common.attributes"/>
      <optional>
        <element name="personname">
          <ref name="db.common.attributes"/>
          <interleave>
            <optional><element name="firstname"><text/></element></optional>
            <optional><element name="surname"><text/></element></optional>
          </interleave>
        </element>
      </optional>
    </element>
  </define>

  <define name="pubdate"><element name="pubdate"><text/></element></define>
  <define name="date"><element name="date"><text/></element></define>

  <define name="copyright">
    <element name="copyright">
      <ref name="db.common.attributes"/>
      <zeroOrMore>
        <element name="year"><data type="gYear" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes"/></element>
      </zeroOrMore>
      <optional><element name="holder"><text/></element></optional>
    </element>
  </define>

  <define name="abstract">
    <element name="abstract">
      <ref name="db.common.attributes"/>
      <optional><ref name="title"/></optional>
      <ref name="blocks"/>
    </element>
  </define>

  <define name="keywordset">
    <element name="keywordset">
      <ref name="db.common.attributes"/>
      <oneOrMore><element name="keyword"><text/></element></oneOrMore>
    </element>
  </define>

  <define name="subjectset">
    <element name="subjectset">
      <ref name="db.common.attributes"/>
      <optional><attribute name="scheme"><data type="string"/></attribute></optional>
      <oneOrMore>
